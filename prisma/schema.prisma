generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "cockroachdb"
  url      = env("DATABASE_URL")
}

model User {
  id       String  @id @default(uuid())
  username String  @db.String(255)
  email    String  @unique @db.String(255)
  image    String? @db.String(255)
}
